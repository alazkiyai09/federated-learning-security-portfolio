    "def verify_signature_simulation(\n",
    "    n_clients: int = 10,\n",
    "    n_forgeries: int = 2\n",
    ") -> dict:\n",
    "    \"\"\"\n",
    "    Simulate ECDSA signature verification.\n",
    "    \n",
    "    Real implementation uses cryptography.io library with:\n",
    "    - SECP256R1 (P-256) elliptic curve\n",
    "    - SHA-256 hashing\n",
    "    - ECDSA signature scheme\n",
    "    \"\"\"\n",
    "    # Simulate verification results\n",
    "    verified = []\n",
    "    rejected = []\n",
    "    \n",
    "    for i in range(n_clients):\n",
    "        # Simulate: first n_forgeries have invalid signatures\n",
    "        is_valid = i >= n_forgeries\n",
    "        \n",
    "        if is_valid:\n",
    "            verified.append(i)\n",
    "        else:\n",
    "            rejected.append(i)\n",
    "    \n",
    "    return {\n",
    "        'n_clients': n_clients,\n",
    "        'n_verified': len(verified),\n",
    "        'n_rejected': len(rejected),\n",
    "        'rejected_ids': rejected\n",
    "    }\n",
    "\n",
    "# Simulate\n",
    "result = verify_signature_simulation(n_clients=10, n_forgeries=2)\n",
    "\n",
