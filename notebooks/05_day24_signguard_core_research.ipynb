    "class ReputationSystem:\n",
    "    \"\"\"\n",
    "    Time-decay reputation system with bonus/penalty.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(\n",
    "        self,\n",
    "        initial_reputation: float = 0.5,\n",
    "        decay_rate: float = 0.05,\n",
    "        honesty_bonus: float = 0.1,\n",
    "        penalty_factor: float = 0.5\n",
    "    ):\n",
    "        self.initial_reputation = initial_reputation\n",
    "        self.decay_rate = decay_rate\n",
    "        self.honesty_bonus = honesty_bonus\n",
    "        self.penalty_factor = penalty_factor\n",
    "        \n",
    "        self.reputations = {}  # client_id â†’ reputation\n",
    "        self.last_update_round = {}  # client_id â†’ round\n",
    "    \n",
    "    def update_reputation(\n",
    "        self,\n",
    "        client_id: int,\n",
    "        anomaly_score: float,\n",
    "        round_num: int,\n",
    "        is_verified: bool = True\n",
    "    ) -> float:\n",
    "        \"\"\"\n",
    "        Update client reputation based on behavior.\n",
    "        \n",
    "        Formula:\n",
    "        1. Apply time decay: new_rep = rep * decay_rate^(rounds_since_update)\n",
    "        2. Apply bonus or penalty based on anomaly score\n",
    "        \"\"\"\n",
    "        # Initialize if new client\n",
    "        if client_id not in self.reputations:\n",
    "            self.reputations[client_id] = self.initial_reputation\n",
    "            self.last_update_round[client_id] = round_num - 1\n",
    "        \n",
    "        rep = self.reputations[client_id]\n",
    "        rounds_since = round_num - self.last_update_round[client_id]\n",
    "        \n",
    "        # Apply time decay\n",
    "        decay = self.decay_rate ** rounds_since\n",
    "        new_rep = rep * decay\n",
    "        \n",
    "        # Apply bonus or penalty\n",
    "        if is_verified:\n",
    "            if anomaly_score < 0.3:  # Low anomaly = honest\n",
    "                new_rep = min(1.0, new_rep + self.honesty_bonus)\n",
    "            elif anomaly_score > 0.7:  # High anomaly = suspicious\n",
    "                penalty = anomaly_score * self.penalty_factor\n",
    "                new_rep = max(0.0, new_rep - penalty)\n",
    "        \n",
    "        # Update state\n",
    "        self.reputations[client_id] = new_rep\n",
    "        self.last_update_round[client_id] = round_num\n",
    "        \n",
    "        return new_rep\n",
    "\n",
    "# Simulate reputation evolution\n",
    "reputation_system = ReputationSystem()\n",
    "\n",
    "# Scenario: 3 clients, 1 is consistently honest, 1 fluctuates, 1 malicious\n",
    "print(\"Layer 3: Time-Decay Reputation Evolution\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "history = {0: [], 1: [], 2: []}\n",
    "\n",
    "for round_num in range(1, 11):\n",
    "    # Client 0: Always honest (low anomaly)\n",
    "    rep0 = reputation_system.update_reputation(0, 0.1, round_num)\n",
    "    history[0].append(rep0)\n",
    "    \n",
    "    # Client 1: Fluctuating (random anomaly)\n",
    "    anomaly_1 = 0.2 + np.random.rand() * 0.4\n",
    "    rep1 = reputation_system.update_reputation(1, anomaly_1, round_num)\n",
    "    history[1].append(rep1)\n",
    "    \n",
    "    # Client 2: Consistently malicious (high anomaly)\n",
    "    rep2 = reputation_system.update_reputation(2, 0.9, round_num)\n",
    "    history[2].append(rep2)\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(12, 6))\n",
    "for client_id, reputations in history.items():\n",
    "    label = f\"Client {client_id} ({'Honest' if client_id == 0 else 'Mixed' if client_id == 1 else 'Malicious'})\"\n",
    "    plt.plot(range(1, 11), reputations, 'o-', label=label, linewidth=2)\n",
    "\n",
    "plt.xlabel('Round Number', fontsize=12)\n",
    "plt.ylabel('Reputation Score', fontsize=12)\n",
    "plt.title('Reputation Evolution Over Rounds', fontsize=14)\n",
    "plt.legend()\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\nFinal Reputations:\")\n",
    "print(f\"  Client 0 (Honest):    {history[0][-1]:.3f}\")\n",
    "print(f\"  Client 1 (Mixed):     {history[1][-1]:.3f}\")\n",
    "print(f\"  Client 2 (Malicious): {history[2][-1]:.3f}\")"
