    "def compute_anomaly_scores(\n",
    "    updates: list,\n",
    "    global_model: np.ndarray,\n",
    "    method: str = 'weighted'\n",
    ") -> dict:\n",
    "    \"\"\"\n",
    "    Compute multi-factor anomaly scores.\n",
    "    \n",
    "    Returns anomaly scores for each client.\n",
    "    \"\"\"\n",
    "    n = len(updates)\n",
    "    scores = {}\n",
    "    \n",
    "    for i, update in enumerate(updates):\n",
    "        # Factor 1: L2 Norm Magnitude\n",
    "        l2_score = np.linalg.norm(update) / (np.linalg.norm(global_model) + 1e-6)\n",
    "        magnitude_score = min(l2_score / 3.0, 1.0)  # Normalize to [0,1]\n",
    "        \n",
    "        # Factor 2: Cosine Similarity Direction\n",
    "        dot = np.dot(update.flatten(), global_model.flatten())\n",
    "        norm_prod = np.linalg.norm(update) * np.linalg.norm(global_model)\n",
    "        cosine_sim = dot / (norm_prod + 1e-6)\n",
    "        direction_score = 1 - cosine_sim  # Low similarity = high anomaly\n",
    "        \n",
    "        # Factor 3: Loss Deviation (simplified)\n",
    "        # In real system: actual training loss\n",
    "        loss_score = abs(np.random.randn()) * 0.2  # Simulated\n",
    "        loss_score = min(loss_score, 1.0)\n",
    "        \n",
    "        # Combine (ensemble)\n",
    "        if method == 'weighted':\n",
    "            combined = 0.4 * magnitude_score + 0.4 * direction_score + 0.2 * loss_score\n",
    "        elif method == 'max':\n",
    "            combined = max(magnitude_score, direction_score, loss_score)\n",
    "        elif method == 'voting':\n",
    "            combined = (\n",
    "                (magnitude_score > 0.7) + \n",
    "                (direction_score > 0.7) + \n",
    "                (loss_score > 0.7)\n",
    "            ) / 3.0\n",
    "        else:\n",
    "            combined = magnitude_score\n",
    "        \n",
    "        scores[i] = {\n",
    "            'magnitude': magnitude_score,\n",
    "            'direction': direction_score,\n",
    "            'loss': loss_score,\n",
    "            'combined': combined\n",
    "        }\n",
    "    \n",
    "    return scores\n",
    "\n",
    "# Simulate\n",
    "np.random.seed(42)\n",
    "n_clients = 10\n",
    "global_model = np.random.randn(10, 10)\n",
    "\n",
    "# Create updates (2 anomalous)\n",
    "updates = []\n",
    "for i in range(n_clients):\n",
    "    if i in [3, 7]:  # Anomalous clients\n",
    "        # Large deviation from global\n",
    "        update = global_model + np.random.randn(10, 10) * 5\n",
    "    else:\n",
    "        # Normal update\n",
    "        update = global_model + np.random.randn(10, 10) * 0.5\n",
    "    updates.append(update)\n",
    "\n",
    "# Compute scores\n",
    "scores = compute_anomaly_scores(updates, global_model)\n",
    "\n",
    "# Display\n",
    "print(\"Layer 2: Multi-Factor Anomaly Detection\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "for client_id, score in scores.items():\n",
    "    status = \"âš ï¸ ANOMALOUS\" if score['combined'] > 0.7 else \"âœ… OK\"\n",
    "    print(f\"Client {client_id}: {score['combined']:.3f} {status}\")\n",
    "    print(f\"  â”œâ”€ Magnitude: {score['magnitude']:.3f}\")\n",
    "    print(f\"  â”œâ”€ Direction: {score['direction']:.3f}\")\n",
    "    print(f\"  â””â”€ Loss: {score['loss']:.3f}\")\n",
    "    print()"
